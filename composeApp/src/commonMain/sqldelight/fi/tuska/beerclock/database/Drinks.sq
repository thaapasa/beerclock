CREATE TABLE drinks (
  id INTEGER PRIMARY KEY NOT NULL,
  time INTEGER NOT NULL,
  name TEXT NOT NULL,
  quantity_liters REAL NOT NULL,
  abv REAL NOT NULL,
  image TEXT NOT NULL
);

CREATE INDEX drinks_time ON drinks(time);

selectAll:
SELECT *
FROM drinks;

insert:
INSERT INTO drinks(time, name, quantity_liters, abv, image)
VALUES (?, ?, ?, ?, ?);

delete:
DELETE FROM drinks
WHERE id = ?;

